        let cart = [];
        let currentFilter = 'todos';

        function renderProducts(filter = 'todos', search = '') {
            const grid = document.getElementById('productsGrid');
            let filtered = products;

            if (filter !== 'todos') {
                if (filter === 'negro') {
                    filtered = products.filter(p => p.color === 'negro');
                } else {
                    filtered = products.filter(p => p.category === filter);
                }
            }

            if (search) {
                filtered = filtered.filter(p => 
                    p.name.toLowerCase().includes(search.toLowerCase()) ||
                    p.code.toLowerCase().includes(search.toLowerCase())
                );
            }

            grid.innerHTML = filtered.map(product => `
                <div class="product-card">
                    <span class="product-code">${product.code}</span>
                    ${product.color === 'negro' ? '<span class="category-badge negro-badge">Negro</span>' : ''}
                    <h3 class="product-name">${product.name}</h3>
                    <div class="product-specs">
                        <div class="spec-item">
                            <span class="spec-label">üìè Dimensiones:</span>
                            <span class="spec-value">${product.size}</span>
                        </div>
                        <div class="spec-item">
                            <span class="spec-label">üì¶ Empaque:</span>
                            <span class="spec-value">${product.pack}</span>
                        </div>
                        <div class="spec-item">
                            <span class="spec-label">üé® Color:</span>
                            <span class="spec-value">${product.color === 'negro' ? 'Negro' : 'Cromado'}</span>
                        </div>
                    </div>
                    <button class="add-to-cart" onclick='addToCart(${JSON.stringify(product)})'>
                        ‚ûï Agregar al Pedido
                    </button>
                </div>
            `).join('');
        }

        function addToCart(product) {
            cart.push(product);
            updateCart();
            
            // Animaci√≥n visual
            const cartIcon = document.querySelector('.cart-icon');
            cartIcon.style.transform = 'scale(1.2)';
            setTimeout(() => cartIcon.style.transform = 'scale(1)', 300);
        }

        function removeFromCart(index) {
            cart.splice(index, 1);
            updateCart();
        }

        function updateCart() {
            const count = document.getElementById('cartCount');
            const items = document.getElementById('cartItems');
            
            count.textContent = cart.length;

            if (cart.length === 0) {
                items.innerHTML = '<div class="empty-cart"><h3>üõí Tu pedido est√° vac√≠o</h3><p>Agrega productos del cat√°logo</p></div>';
            } else {
                items.innerHTML = cart.map((item, index) => `
                    <div class="cart-item">
                        <div class="cart-item-info">
                            <div class="cart-item-name">${item.name}</div>
                            <div class="cart-item-code">${item.code} - ${item.size}</div>
                        </div>
                        <button class="remove-item" onclick="removeFromCart(${index})">‚úï</button>
                    </div>
                `).join('');

                items.innerHTML += `
                    <div style="margin-top: 20px; padding-top: 20px; border-top: 2px solid #667eea;">
                        <h3 style="color: #2c3e50; margin-bottom: 10px;">Total de productos: ${cart.length}</h3>
                        <button class="add-to-cart" onclick="downloadOrder()">üìÑ Descargar Pedido en PDF</button>
                    </div>
                `;
            }
        }

        function downloadOrder() {
            const { jsPDF } = window.jspdf;
            const doc = new jsPDF();
            
            // Header
            doc.setFillColor(44, 62, 80);
            doc.rect(0, 0, 210, 40, 'F');
            
            doc.setTextColor(255, 255, 255);
            doc.setFontSize(24);
            doc.setFont(undefined, 'bold');
            doc.text('MECOB', 105, 15, { align: 'center' });
            
            doc.setFontSize(12);
            doc.setFont(undefined, 'normal');
            doc.text('Art√≠culos de Alambre - Quincalla y Hogar', 105, 25, { align: 'center' });
            doc.text('PEDIDO - Cat√°logo 2026', 105, 33, { align: 'center' });
            
            // Date
            const now = new Date();
            const dateStr = now.toLocaleDateString('es-ES', { 
                year: 'numeric', 
                month: 'long', 
                day: 'numeric',
                hour: '2-digit',
                minute: '2-digit'
            });
            
            doc.setTextColor(0, 0, 0);
            doc.setFontSize(10);
            doc.setFont(undefined, 'italic');
            doc.text(`Fecha de pedido: ${dateStr}`, 20, 50);
            
            // Products
            doc.setFont(undefined, 'bold');
            doc.setFontSize(14);
            doc.text('Lista de Productos:', 20, 60);
            
            let yPos = 70;
            doc.setFontSize(10);
            
            cart.forEach((item, index) => {
                // Check if we need a new page
                if (yPos > 270) {
                    doc.addPage();
                    yPos = 20;
                }
                
                // Product number and code
                doc.setFont(undefined, 'bold');
                doc.setTextColor(102, 126, 234);
                doc.text(`${index + 1}. ${item.code}`, 20, yPos);
                
                // Product name
                doc.setTextColor(0, 0, 0);
                doc.setFont(undefined, 'bold');
                yPos += 6;
                const lines = doc.splitTextToSize(item.name, 170);
                doc.text(lines, 25, yPos);
                yPos += (lines.length * 5);
                
                // Details
                doc.setFont(undefined, 'normal');
                doc.setTextColor(100, 100, 100);
                yPos += 5;
                doc.text(`   ‚Ä¢ Dimensiones: ${item.size}`, 25, yPos);
                yPos += 5;
                doc.text(`   ‚Ä¢ Empaque: ${item.pack}`, 25, yPos);
                yPos += 5;
                doc.text(`   ‚Ä¢ Color: ${item.color === 'negro' ? 'Negro' : 'Cromado'}`, 25, yPos);
                
                // Separator line
                yPos += 8;
                doc.setDrawColor(200, 200, 200);
                doc.line(20, yPos, 190, yPos);
                yPos += 8;
            });
            
            // Footer with total
            if (yPos > 250) {
                doc.addPage();
                yPos = 20;
            }
            
            yPos += 5;
            doc.setFillColor(102, 126, 234);
            doc.roundedRect(20, yPos, 170, 15, 3, 3, 'F');
            
            doc.setTextColor(255, 255, 255);
            doc.setFontSize(12);
            doc.setFont(undefined, 'bold');
            doc.text(`TOTAL DE PRODUCTOS: ${cart.length}`, 105, yPos + 10, { align: 'center' });
            
            // Contact info footer
            yPos += 25;
            doc.setTextColor(150, 150, 150);
            doc.setFontSize(9);
            doc.setFont(undefined, 'italic');
            doc.text('MECOB - Art√≠culos de Alambre', 105, yPos, { align: 'center' });
            doc.text('www.mecob.com', 105, yPos + 5, { align: 'center' });
            
            // Save the PDF
            const fileName = `Pedido-MECOB-${now.toISOString().split('T')[0]}.pdf`;
            doc.save(fileName);
        }

        function toggleCart() {
            const modal = document.getElementById('cartModal');
            modal.style.display = modal.style.display === 'flex' ? 'none' : 'flex';
        }

        // Event Listeners
        document.querySelectorAll('.filter-btn').forEach(btn => {
            btn.addEventListener('click', () => {
                document.querySelectorAll('.filter-btn').forEach(b => b.classList.remove('active'));
                btn.classList.add('active');
                currentFilter = btn.dataset.category;
                renderProducts(currentFilter, document.getElementById('searchBox').value);
            });
        });

        document.getElementById('searchBox').addEventListener('input', (e) => {
            renderProducts(currentFilter, e.target.value);
        });

        // Click outside modal to close
        document.getElementById('cartModal').addEventListener('click', (e) => {
            if (e.target.id === 'cartModal') toggleCart();
        });

        // Initial render
        renderProducts();
    </script>
</body>
</html>